<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/manage"
    tools:context="com.example.saibot.test1.DashboardActivity"
    android:padding="0dp" >


    <Button
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="10dp"
        android:padding="10dp"
        android:background="@null"
        android:scaleType="centerCrop"
        android:id="@+id/menuButton"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:onClick="menu_clicked" />

    <!--<Button-->
    <!--android:layout_width="100dp"-->
    <!--android:layout_height="100dp"-->
    <!--android:layout_marginTop="250dp"-->
    <!--android:layout_marginLeft="250dp"-->
    <!--android:background="@null"-->
    <!--android:scaleType="centerCrop"-->
    <!--android:id="@+id/see_logs_button"-->
    <!--android:layout_alignParentTop="true"-->
    <!--android:layout_alignParentStart="true"-->
    <!--android:onClick="logs_clicked" />-->

    <FrameLayout
        android:layout_marginLeft="-283dp"
        android:layout_width="283dp"
        android:layout_height="fill_parent"
        android:id="@+id/menuLayout">

        <ImageView
            android:layout_width="283dp"
            android:layout_height="fill_parent"
            android:id="@+id/menuView"
            android:src="@drawable/menu_smal"/>

        <Button
            android:layout_width="45dp"
            android:layout_height="40dp"
            android:text=""
            android:id="@+id/menu_menu_button"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="5dp"
            android:background="@null"
            android:onClick="menu_toggle"
            android:layout_gravity="right" />
    </FrameLayout>

    <Button
        android:layout_width="380dp"
        android:layout_height="230dp"
        android:layout_marginTop="300dp"
        android:layout_marginLeft="0dp"
        android:background="@null"
        android:scaleType="centerCrop"
        android:id="@+id/Manage_Guests"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:onClick="go_Guests" />

    <Button
        android:layout_width="370dp"
        android:layout_height="200dp"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="0dp"
        android:background="@null"
        android:scaleType="centerCrop"
        android:id="@+id/goRfid"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:onClick="go_RFID" />

</RelativeLayout>
